{
    "name": "aws-serverless-rest-api-node-js-dynamo-db",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "prepare": "tsc --noEmit",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\"",
        "lint": "eslint \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" --fix",
        "test": "jest"
